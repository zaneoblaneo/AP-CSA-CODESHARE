package zaneoblaneo.magPie116;

public class StringHandling {
	public static void handleString(String[] sentence){
		for(String s : sentence){
			switch (s.toUpperCase()) {
			case "END":  parseLike(sentence); break;
			case "LIKE" : parseLike(sentence); break;
			case "LOVE" : parseLike(sentence); break;
			case "ENJOY" : parseLike(sentence); break;
			case "<3" : parseLike(sentence); break;
			case "MAY" : parseQuestion(sentence); break;
			case "CAN" : parseQuestion(sentence); break;

			default:
				break;
			}
		}
	}
	public static void parseQuestion(String[] sentence){
		
	}
	public static void parseLike(String[] sentence){
		
	}
}
