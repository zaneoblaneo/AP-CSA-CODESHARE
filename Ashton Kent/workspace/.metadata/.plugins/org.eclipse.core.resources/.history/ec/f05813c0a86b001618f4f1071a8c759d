package zaneoblaneo.ifItIsRaining;

import java.util.Hashtable;

public class WeatherIndex {
	
	public static void makeWeatherIndex(){
		Hashtable<Integer, String> weatherIndex = new Hashtable<Integer, String>();
		
		weatherIndex.put(0,"tornado");
		weatherIndex.put(1,  "tropical storm");
		weatherIndex.put(2, "hurricane");
		weatherIndex.put(3, "severe thunderstorms");
		weatherIndex.put(4, "thunderstorms");
		weatherIndex.put(5, "mixed rain and snow");
		weatherIndex.put(6, "mixed raind and sleet");
		weatherIndex.put(7, "mixed snow and sleet");
		weatherIndex.put(8, "freezing drizzle");
		weatherIndex.put(9, "drizzle");
		
	}
	
}
