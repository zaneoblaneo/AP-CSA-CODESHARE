public class MainClass {
	public static void main(String[] args){
		String[] SASentence;
		String sentence;
		Scanner scanner = new Scanner(System.in);
		sentence = scanner.nextLine();
		SASentence = sentence.split(" ");
		StringHandling.handleString(SASentence);
	}
}
