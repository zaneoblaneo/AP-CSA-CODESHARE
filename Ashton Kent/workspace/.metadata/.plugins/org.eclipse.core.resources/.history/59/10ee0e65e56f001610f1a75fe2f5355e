package zaneoblaneo.magPie116;

import zaneoblaneo.utils.*;

public class StringHandling {
	public static void handleString(String[] sentence){
		switch (sentence[0].toUpperCase()) {
		case "I": parseI(sentence); break;
		case "YOU" : parseYou(sentence); break;
		case "U" : parseYou(sentence); break;
		case "HE" : parseHe(sentence); break;
		case "SHE" : parseShe(sentence); break;
		
		default: Print.println("I don't understand!"); break;
		}
	}
	public static void parseQuestion(String[] sentence){
		
	}
	public static void parseLike(String[] sentence){
		
	}
}
